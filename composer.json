{
    "name": "honeybadger-io/honeybadger-laravel",
    "description": "Honeybadger Laravel integration",
    "keywords": [
        "logging",
        "debugging",
        "monitoring",
        "errors",
        "exceptions",
        "honeybadger-io",
        "honeybadger-php"
    ],
    "homepage": "https://github.com/honeybadger-io/honeybadger-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "TJ Miller",
            "email": "oss@tjmiller.co",
            "homepage": "https://tjmiller.co",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1|^7.2",
        "honeybadger-io/honeybadger-php": "^1.2",
        "illuminate/console": "^5.5|^5.6",
        "illuminate/support": "^5.5|^5.6"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.10",
        "larapack/dd": "^1.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "^3.5|^3.6|^3.7",
        "phpunit/phpunit": "^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Honeybadger\\HoneybadgerLaravel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Honeybadger\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "phpunit --coverage-html coverage",
        "styles:lint": "vendor/bin/php-cs-fixer fix --dry-run --diff",
        "styles:fix": "vendor/bin/php-cs-fixer fix"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Honeybadger\\HoneybadgerLaravel\\HoneybadgerServiceProvider"
            ],
            "aliases": {
                "Honeybadger": "Honeybadger\\HoneybadgerLaravel\\Facades\\Honeybadger"
            }
        }
    }
}
